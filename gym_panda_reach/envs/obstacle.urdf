<?xml version="1.0" ?>
<robot name="obstacle">
  <link name="baseLink">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>  <!-- 0.65 0 0.2调整xyz属性来放置障碍物在桌子的中间 -->
      <geometry>
        <box size="0.1 0.1 0.1"/>  <!-- 调整size属性来让障碍物比桌子小 -->
      </geometry>
      <material name="green">
        <color rgba="0.9 0.1 0.0 1"/>
      </material>
    </visual>
  </link>
</robot>
